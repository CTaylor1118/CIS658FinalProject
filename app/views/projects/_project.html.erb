<div projectid="<%= project.id %>"class="project grid-item">
  <div class="thumbnail">
    <img src="<%= project.image %>" alt="<%= project.name %>" class="img-thumbnail"/>
    <div class="caption">
      <div>
        <span class="projectname pull-left"><%= project.name %></span>
        <button type="button" class="btn btn-secondary js-rename pull-right">
          <span class="glyphicon glyphicon-pencil pull-right" aria-hidden="true"></span>
        </button>
      </div>
      <hr>
      <div class="btn-group btn-group-default btn-group-justified" role="group">
        <a href="/projects/<%= project.id %>/edit" class="btn btn-primary text-center" role="button" data-no-turbolink>
          <span class="glyphicon glyphicon-edit" aria-hidden="true"></span>
        </a> 
        <a href="#" onclick="filepicker.setKey('AszT4VzZRMm5R0BtsKYVoz');filepicker.exportFile('<%= project.image %>',{mimetype:'image/jpeg',suggestedFilename: 'newFile', service: 'COMPUTER'},function(Blob){console.log(Blob.url);});" class="btn btn-success text-center" role="button" data-no-turbolink>
          <span class="glyphicon glyphicon-download-alt" aria-hidden="true"></span> 
        </a>
        <a href="/projects/<%= project.id %>" class="btn btn-danger text-center" role="button" data-method="delete" data-remote="true" data-no-turbolink>
          <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
        </a>
      </div>
    </div>
  </div>
</div>